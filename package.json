{
  "name": "clasp-simple-template",
  "version": "0.0.1",
  "description": "template for Google Apps Script in Local Env (simple style)",
  "main": "app.js",
  "scripts": {
    "build": "node build.js",
    "build:watch": "nodemon --watch src --ext ts --exec \"tsc --noEmit && node build.js\"",
    "push": "node build.js && clasp push",
    "push:watch": "clasp push --watch",
    "tsc": "tsc --noEmit",
    "test": "jest --silent=true --coverage",
    "test:log": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luthpg/clasp-simple-template.git"
  },
  "keywords": [
    "clasp",
    "GoogleAppsScript"
  ],
  "author": "luthpg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luthpg/clasp-simple-template/issues"
  },
  "homepage": "https://github.com/luthpg/clasp-simple-template#readme",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.59",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "esbuild": "^0.17.14",
    "esbuild-gas-plugin": "^0.5.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
